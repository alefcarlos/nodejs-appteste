<form class="form-horizontal" action="/contatos/<%- id %>/confirmDelete" method="post">
  <div class="form-group">
    <label class="col-sm-2 control-label">Nome</label>
    <div class="col-sm-10">
      <p class="form-control-static"><%- contato.nome -%></p>
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-2 control-label">E-mail</label>
    <div class="col-sm-10">
      <p class="form-control-static"><%- contato.email -%></p>
    </div>
  </div>

  <button class="btn btn-danger btn-raised" type="submit">Deletar</button>
  <a  class="btn btn-default btn-raised" href="/contatos/<%- id %>/edit">Editar</a>
  <a  class="btn btn-default btn-raised" href="/contatos">Voltar</a>
</form>
