<form class="form-horizontal" action="/contatos/<%- id %>/update" method="post">

  <div class="form-group label-floating">
      <label class="control-label" for="email">digite o e-mail do contato</label>
      <input type="email" class="form-control" name="email" id="email" value="<%- contato.email -%>">
  </div>

  <div class="form-group label-floating">
      <label class="control-label" for="email">digite o nome do contato</label>
      <input type="text" class="form-control" name="nome" id="nome" value="<%- contato.nome -%>">
  </div>

  <button class="btn btn-success btn-raised" type="submit">Atualizar</button>
  <a  class="btn btn-default btn-raised" href="/contatos">Voltar</a>
</form>
